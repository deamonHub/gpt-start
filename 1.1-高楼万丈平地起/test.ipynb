   "source": [
    "from matplotlib.font_manager import fontManager\n",
    "\n",
    "# 获取系统中所有可用字体\n",
    "fonts = fontManager.ttflist  # TrueType字体列表\n",
    "otf_fonts = fontManager.afmlist  # OpenType/CFF字体列表\n",
    "\n",
    "# 打印所有TrueType字体名称\n",
    "for font in fonts:\n",
    "    print(font.name)"
